#cambiar de decimal a binario     
import math
        Dividendo=int(input("ingresa Dividendo:\n"));
        divisor = 2;
        q=math.floor(Dividendo/divisor)
        r=Dividendo%divisor
    print("el cociente es: "+str(q))
    print("el residuo es: "+str(r))
